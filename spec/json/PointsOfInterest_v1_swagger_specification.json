    "categories": {
      "name": "categories",
      "description": "category of the location. \n\nMultiple value can be selected using a comma i.e. SIGHTS, SHOPPING\n",
      "in": "query",
      "required": false,
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
